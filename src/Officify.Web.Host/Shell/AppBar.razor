<MudAppBar>
    <MudIconButton Icon="@Icons.Material.Filled.Menu" OnClick="@HandleMenuButtonClick" UserAttributes="@MenuAttributes">
        Officify
    </MudIconButton>
</MudAppBar>

@code {
    [Parameter]
    public EventCallback OnToggleDrawer { get; set; }

    private Dictionary<string, object?> MenuAttributes { get; } = new()
    {
        {"aria-label", "menu"},
        {"role", "button"}
    };

    private async Task HandleMenuButtonClick()
    {
        await OnToggleDrawer.InvokeAsync();
    }
}